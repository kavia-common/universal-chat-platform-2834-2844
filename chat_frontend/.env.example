# Example environment variables for real backend integration
# Copy to .env and set appropriate values. Do not commit real secrets.

# REST API base URL, e.g. https://api.example.com
REACT_APP_API_URL=

# WebSocket URL, e.g. wss://ws.example.com/chat
REACT_APP_WS_URL=

# Feature flags
REACT_APP_ENABLE_MOCKS=true
